# https://github.com/graphql-java/graphql-java/releases/tag/v9.0

directive @BLA on FIELD_DEFINITION


type User {
  id: ID!
  name: String!
}

type Rating {
  id: ID!
  user: User!
  stars: Int!
}

interface BeerNode {
  id: ID!
}

type Beer implements BeerNode{
  id: ID!
  name: String!
  price: String!

  ratings: [Rating!]! @BLA
}



type Query {
  beers: [Beer!]!
}